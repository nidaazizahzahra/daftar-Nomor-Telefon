a = {
    "Jane Doe": "+27 555 5367",
    "John Smith": "+27 555 6254",
    "Bob Stone": "+27 555 5689"
    }

for key, val in a.items():
    print("%s : %s" % (key,val))

print(a["Jane Doe"])
a["Jane Doe"] = "+27 555 1024"
print(a["Jane Doe"])

a.update({"anna cooper": "+27 555 3237"})
print(a)

print("The number Bob Stone is %s" % a["Bob Stone"])


for k in a.keys():
    print (k)

for v in a.values():
    print (v)

for k,v in a.items():
    print(k,v)
